# OLX Search URL - Replace with your desired search
OLX_SEARCH_URL=https://www.olx.pl/oferty/q-iphone/

# Discord Webhook URL - Get this from your Discord server settings
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# Polling interval in seconds (how often to check for new listings)
POLL_INTERVAL=60

# User agent string (optional, defaults to a Chrome user agent)
USER_AGENT="Mozilla/5.0 (Windows NT 10.0; Win64; x64) AppleWebKit/537.36 Chrome/120 Safari/537.36"

# Database file path (optional, defaults to ./seen.sqlite)
DB_PATH=./seen.sqlite
